{
  "name": "plant-manager-backend",
  "version": "1.0.0",
  "main": "bin/www",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "start:build": "npm-run-all build:clean start",
    "start:watch": "nodemon --exec yarn start:build",
    "build": "babel ./src -d ./dist",
    "build:clean": "npm-run-all clean build",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:strict": "eslint . --max-warnings 0",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "dotenv": "^16.0.1",
    "express": "~4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "mqtt": "^4.3.7",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize": "^6.21.3",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@babel/register": "^7.18.9",
    "@faker-js/faker": "^7.3.0",
    "eslint": "^8.20.0",
    "nodemon": "^2.0.19",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.4.1"
  }
}
