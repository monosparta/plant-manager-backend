{
    "env": {
        "node": true,
        "es6": true,
        "jest": true
        
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "eqeqeq": "error",
        "no-unused-vars": "warn",
        "no-trailing-spaces": "error",
        "camelcase": "warn",
        "no-var": "error",
        "no-useless-return": "error",
        "no-else-return": "error",
        "no-empty": "error",
        "space-before-function-paren": "error",
        "space-in-parens": "error",
        "space-before-blocks": "error",
        "comma-dangle": "error",
        "no-duplicate-imports": "error"
    }
}
